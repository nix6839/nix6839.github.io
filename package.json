{
  "name": "nix6839.github.io",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "format": "prettier --write \"**/*.{astro,ts,js,cjs,css,md,json,yml,yaml}\"",
    "format:check": "prettier --check \"**/*.{astro,ts,js,cjs,css,md,json,yml,yaml}\"",
    "lint": "eslint \"**/*.{ts,astro}\" && pnpm astro check && pnpm tsc",
    "fetch-fonts": "fetch-pretendard --font-output-dir ./public/fonts --css-output-path ./src/styles/pretendardvariable-dynamic-subset.css --css-url 'https://cdn.jsdelivr.net/gh/orioncactus/pretendard@v1.3.8/dist/web/variable/pretendardvariable-dynamic-subset.css'"
  },
  "dependencies": {
    "@astrojs/tailwind": "5.0.0",
    "astro": "3.0.2",
    "tailwindcss": "3.3.3"
  },
  "devDependencies": {
    "@astrojs/check": "0.2.0",
    "@nix6839/prettier-config": "2.0.2",
    "@types/eslint": "8.44.2",
    "@typescript-eslint/eslint-plugin": "6.5.0",
    "@typescript-eslint/parser": "6.5.0",
    "eslint": "8.48.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-astro": "0.29.0",
    "fetch-pretendard": "workspace:^",
    "prettier": "3.0.3",
    "prettier-plugin-astro": "0.12.0",
    "prettier-plugin-tailwindcss": "0.5.3",
    "typescript": "5.2.2"
  },
  "packageManager": "pnpm@8.7.0+sha256.aef8d26bc17616c60dcb15d2db30803766051a2720edcc3bd60b87fb8c925470"
}
